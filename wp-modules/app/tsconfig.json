{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"allowSyntheticDefaultImports": true,
		"jsx": "preserve",
		"target": "esnext",
		"module": "esnext",
		"lib": [ "dom", "esnext" ],
		"moduleResolution": "node",
		"esModuleInterop": true,
		"noEmit": true
	},
	"skipLibCheck": true,
	"include": [ "js/src/**/*" ]
}
